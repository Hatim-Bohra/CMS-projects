{
    "kind": "collectionType",
    "collectionName": "articles",
    "info": {
        "singularName": "article",
        "pluralName": "articles",
        "displayName": "Article",
        "description": "Blog articles"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "title": {
            "type": "string",
            "required": true
        },
        "content": {
            "type": "richtext"
        },
        "slug": {
            "type": "uid",
            "targetField": "title"
        },
        "category": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::category.category",
            "inversedBy": "articles"
        }
    }
}